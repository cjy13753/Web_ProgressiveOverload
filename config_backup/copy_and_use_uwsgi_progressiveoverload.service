[Unit]
Description=uWSGI instance to serve ProgressiveOverload
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/progressiveoverload
Environment=PATH=/home/ubuntu/progressiveoverload/venv/bin DATABASE_URL=postgresql://ubuntu:77482295@localhost:5432/ubuntu SECRET_KEY=somethingcomplex
ExecStart=/home/ubuntu/progressiveoverload/venv/bin/uwsgi --ini uwsgi.ini

[Install]
WantedBy=multi-user.target